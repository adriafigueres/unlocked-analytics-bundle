{
    "name": "m10c/unlocked-analytics-bundle",
    "description": "Analytics server bundle for Symfony",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "symfony/config": "^7.0",
        "symfony/dependency-injection": "^6.4",
        "symfony/http-kernel": "^6.3",
        "symfony/routing": "^7.0",
        "doctrine/persistence": "^3.3",
        "ramsey/uuid-doctrine": "^2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.52",
        "symfony/phpunit-bridge": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "M10c\\UnlockedAnalyticsBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "M10c\\UnlockedAnalyticsBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "cs": "php vendor/bin/php-cs-fixer fix",
        "phpunit": "php vendor/bin/simple-phpunit",

        "test": [
            "@phpunit"
        ]
    }
}
